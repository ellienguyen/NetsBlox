{
  "name": "netsblox",
  "version": "0.7.1",
  "description": "Visual programming environment with networking support",
  "scripts": {
    "start": "node bin/netsblox.js",
    "dev": "node node_modules/nodemon/bin/nodemon src/server.js",
    "testmon": "node node_modules/nodemon/bin/nodemon node_modules/mocha/bin/mocha --recursive test/server",
    "test": "node node_modules/mocha/bin/mocha --recursive test/server",
    "test-client": "node test/client/integration.js",
    "test-cover": "node node_modules/istanbul/lib/cli.js --hook-run-in-context cover node_modules/mocha/bin/_mocha -- -R spec test/server/*.js",
    "postinstall": "node bin/build.js"
  },
  "engines": {
      "node": ">= 4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/NetsBlox/NetsBlox.git"
  },
  "author": "Brian Broll",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/NetsBlox/server/issues"
  },
  "homepage": "https://github.com/NetsBlox/NetsBlox",
  "dependencies": {
    "async": "^2.0.0-rc.2",
    "body-parser": "^1.12.4",
    "cache-manager": "^2.0.0",
    "chalk": "^1.1.1",
    "concat": "^1.0.0",
    "cookie-parser": "^1.3.5",
    "cordova": "^6.1.1",
    "cordova-lib": "^6.1.1",
    "debug": "^2.1.3",
    "dotenv": "^2.0.0",
    "express": "^4.12.3",
    "fs-extra": "^0.26.7",
    "geolib": "^2.0.18",
    "jade": "^1.9.2",
    "jsonwebtoken": "^6.2.0",
    "just.randomstring": "^0.1.1",
    "lodash": "^4.8.2",
    "mongodb": "^2.0.33",
    "nodemailer": "^2.3.0",
    "nodemailer-markdown": "^1.0.0",
    "nop": "^1.0.0",
    "project-name-generator": "^2.1.2",
    "ramda": "^0.20.1",
    "request": "^2.63.0",
    "rimraf": "^2.4.3",
    "socket.io": "^1.3.5",
    "temp-units-conv": "^0.1.1",
    "vantage": "^1.5.1",
    "vantage-repl": "^1.1.4",
    "ws": "^1.0.1",
    "xml2js": "^0.4.10",
    "yargs": "^4.4.0"
  },
  "devDependencies": {
    "expect": "^1.6.0",
    "istanbul": "^0.4.2",
    "mocha": "^2.2.1",
    "mocha-phantomjs": "^4.0.2",
    "nodemon": "^1.3.7",
    "sinon": "^1.16.1",
    "supertest": "^1.0.1",
    "uglify-js": "^2.6.2"
  }
}
